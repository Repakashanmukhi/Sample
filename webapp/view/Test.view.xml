<mvc:View controllerName="sample.controller.Test"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page">
        <subHeader>
            <Bar>
                <contentLeft>
                    <Text id="Text"/>
                </contentLeft>
            </Bar>
        </subHeader>
    <List id="employeeList" items="{/event}" itemPress="onNavigation">
        <items>
            <StandardListItem 
                title="{FirstName} {LastName}" 
                description="{Email}" 
                type="Navigation" >
            </StandardListItem> 
        </items> 
    </List>
    </Page>
</mvc:View>

